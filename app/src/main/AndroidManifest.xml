<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myfirstapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/miwok"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".FamilyMembersActivity"
            android:label="@string/Familymembers" />

         <activity android:name=".ColorsActivity"
            android:label="@string/colors"
        />

         <activity android:name=".PharsesActivity"
             android:label="@string/phrases"

             />

        <activity android:name=".NumbersActivity"
            android:label="@string/numbers"
             />
        
    </application>

</manifest>